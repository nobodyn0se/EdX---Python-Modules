    "from turtle import *\n",
    "from random import randint\n",
    "x = 1\n",
    "\n",
    "while x < 400:\n",
    "    r = randint(0,255)\n",
    "    g = randint(0,255) \n",
    "    b = randint(0,255)\n",
    " \n",
    "   \n",
    "    pencolor(r,g,b)\n",
    "    fd(50 + x)\n",
    "    rt(90.991)\n",
    "    x = x+1\n",
    "    \n",
    "exitonclick()"
